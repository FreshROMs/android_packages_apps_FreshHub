<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:ordering="sequentially">
    <objectAnimator
        android:duration="200"
        android:propertyName="strokeAlpha"
        android:valueFrom="1.0"
        android:valueTo="1.0" />
    <objectAnimator
        android:duration="2"
        android:propertyName="strokeAlpha"
        android:valueFrom="1.0"
        android:valueTo="0" />
</set>